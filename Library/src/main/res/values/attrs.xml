<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--标题栏-->
    <declare-styleable name="TitleBar">
        <attr name="isShowBack" format="boolean" />
        <attr name="titleText" format="string" />
        <attr name="rightText" format="string" />
    </declare-styleable>

    <!--多样式布局 自定义属性-->
    <declare-styleable name="stfStatefulLayout">
        <attr name="stfAnimationEnabled" format="boolean" />
        <attr name="stfInAnimation" format="reference" />
        <attr name="stfOutAnimation" format="reference" />
    </declare-styleable>
    <declare-styleable name="SimpleSwitch">
        <attr name="switch_bottom_layer_off_color" format="color" />
        <attr name="switch_bottom_layer_on_color" format="color" />
        <attr name="switch_circle_color" format="color" />
    </declare-styleable>

</resources>